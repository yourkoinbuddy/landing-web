{
  "name": "koinbuddy-landing",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "main": "index.html",
  "scripts": {
    "build": "sass scss/styles.scss styles.css --style compressed --source-map --embed-sources --no-error-css",
    "build:prod": "sass scss/styles.scss styles.css --style compressed --no-source-map --no-error-css",
    "dev": "npm-run-all build serve watch",
    "watch": "nodemon -e html,js,scss -x \"npm run build\"",
    "serve": "serve",
    "start": "npm-run-all dev"
  },
  "devDependencies": {
    "nodemon": "^3.1.9",
    "npm-run-all": "^4.1.5",
    "sass": "^1.85.1",
    "serve": "^14.2.4"
  },
  "browserslist": [
    "defaults"
  ]
}
